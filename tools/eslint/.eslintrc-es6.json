{
    "rules": {
        "arrow-parens": [2, "always"],
        "space-before-function-paren": ["error", {
            "anonymous": "never",
            "named": "never",
            "asyncArrow": "always"
        }],
        "one-var": ["error", { "const": "never" }],
        "indent": ["error", 4, {
            "SwitchCase": 1,
            "ObjectExpression": 1,
            "MemberExpression": 1,
            "CallExpression": {
                "arguments": 1
            }
        }]
    }
}
